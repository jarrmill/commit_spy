{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name":"server",
      "image":"jarrmill/commit-spy",
      "hostname": "api",
      "essential": false,
      "memory": 256
    },
    {
      "name":"nginx",
      "image": "jarrmill/commit-spy-nginx",
      "hostname":"router",
      "essential": true,
      "memory": 128,
      "portMappings": [
        {
          "hostPort":80,
          "containerPort":80
        }
      ],
      "links": ["server"]
    }
  ]
}